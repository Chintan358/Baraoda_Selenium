package oops;

class Calc
{
	public void msg()
	{
		System.out.println("displaying message....");
	}
	
	public void square(int a)
	{
		int sq = a*a;
		System.out.println(sq);
	}
}

public class O003_MethodManipulation
{
	public static void main(String[] args)
	{
		
		Calc c = new Calc();
		c.msg();
		c.square(10);
		
		
	}
}
